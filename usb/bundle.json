{
  "name": "usb_device_driver",
  "description": "drivers adapter",
  "component": {
    "name": "usb_device_driver",
    "subsystem": "hdf",
    "syscap": [ "" ],
    "adapter_system_type": ["standard", "small"],
    "rom": "1000KB",
    "ram": "1000KB",
    "deps": {
      "components": [
        "developer_test",
        "hilog",
        "utils_base"
      ],
      "third_part": [
        "bounds_checking_function"
      ]
    },
    "build": {
      "sub_component": [
        "//drivers/peripheral/usb:usb_entry"
      ],
      "test": [
        "//drivers/peripheral/usb:usb_test_entry"
      ],
      "inner_kits": [
        {
          "name": "//drivers/peripheral/usb/hal/client:usbd_client",
          "header": {
            "header_files": [
              "usb_errors.h",
              "usb_info.h",
              "usb_param.h",
              "usbd_client.h",
              "usbd_subscriber.h",
              "usbd_type.h"
            ],
            "header_base": "//drivers/peripheral/usb/hal/client/include"
          }
        }
      ]
    }
  }
}
